<maps:MapOverlay
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="AXToolbox.MapViewer.TagOverlay" 
    xmlns:maps="clr-namespace:AXToolbox.MapViewer"
    IsHitTestVisible="False"
    >
    <Grid>
        <!--<Line X1="0" Y1="0" X2="10" Y2="10" Stroke="Black" StrokeThickness="2"></Line>-->
        <!--<Rectangle Width="10" Height="10" Fill="Black" HorizontalAlignment="Left" VerticalAlignment="Top"></Rectangle>-->
        <Path Height="10" Width="10" Fill="Black" Data="M0,0 L10,5 L5,10" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0"/>
        <Border Name="Shape" BorderBrush="Black" BorderThickness="1.5" CornerRadius="10" Background="Lime" Margin="5,5,0,0"/>
        <TextBlock Name="Text" HorizontalAlignment="Center" MinHeight="23" MinWidth="23" Padding="4" Margin="5,5,0,0">Label</TextBlock>
    </Grid>
</maps:MapOverlay>