<maps:MapOverlay
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="AXToolbox.MapViewer.TargetOverlay" 
    xmlns:maps="clr-namespace:AXToolbox.MapViewer"
    IsHitTestVisible="False"
    >
    <Grid>
        <!--<Line X1="0" Y1="0" X2="10" Y2="10" Stroke="Black" StrokeThickness="2"></Line>-->
        <!--<Rectangle Width="10" Height="10" Fill="Black" HorizontalAlignment="Left" VerticalAlignment="Top"></Rectangle>-->
        <Path Name="Shape" Height="30" Width="30" Fill="Red" Margin="0" Opacity=".5" Stretch="UniformToFill"
              Data="M40,0 L60,0 L60,40 L100,40 L100,60 L60,60 L60,100 L40,100 L40,60 L0,60 L0,40 L40,40 L 40,0" />
        <Path Height="1" Width="1" Stroke="Black"  Data="M-1,0 L1,0 M0,-1 L0,1"></Path>
        <!--<Border Name="Shape" BorderBrush="Black" BorderThickness="1.5" CornerRadius="10" Background="Lime" Margin="5,5,0,0"/>-->
        <TextBlock Name="Text" HorizontalAlignment="Left" MinHeight="23" MinWidth="23" Padding="4" Margin="35,20,0,0">Label</TextBlock>
    </Grid>
</maps:MapOverlay>